{
  "expo": {
    "name": "running_well",
    "slug": "running-well",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/running-well1.png",
    "scheme": "runningwell",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyAIQRRLo_O4G94kzvbwwww5N3TBvNXYZtM"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to track your running routes and distance.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to track your running routes even when the app is in the background.",
        "NSLocationAlwaysUsageDescription": "This app needs access to your location to track your running routes even when the app is in the background."
      },
      "bundleIdentifier": "com.runningwell.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/running-well1.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/running-well1.png"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAIQRRLo_O4G94kzvbwwww5N3TBvNXYZtM"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "package": "com.runningwell.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/running-well1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Running Well to use your location to track your runs.",
          "locationAlwaysPermission": "Allow Running Well to use your location to track your runs in the background.",
          "locationWhenInUsePermission": "Allow Running Well to use your location to track your runs."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/running-well1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f4f16015-1b45-4d20-99e3-49fc16fe9601"
      }
    },
    "owner": "v6core"
  }
}
